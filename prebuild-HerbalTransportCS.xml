    <Project frameworkVersion="v4_6" name="Herbal3d.HerbalTransportCS" path="addon-modules/HerbalTransportCS/HerbalTransportCS" type="Library">
      <Configuration name="Debug">
        <Options>
          <OutputPath>../../../bin/</OutputPath>
          <AllowUnsafe>true</AllowUnsafe>
        </Options>
      </Configuration>
      <Configuration name="Release">
        <Options>
          <OutputPath>../../../bin/</OutputPath>
          <AllowUnsafe>true</AllowUnsafe>
        </Options>
      </Configuration>

      <ReferencePath>../../../bin/</ReferencePath>
      <Reference name="System"/>
      <Reference name="System.IO"/>
      <Reference name="System.Linq"/>
      <Reference name="System.Collections.Concurrent"/>
      <Reference name="System.Threading"/>
      <Reference name="System.Threading.Tasks"/>
      <Reference name="System.Reflection"/>
      <Reference name="System.Security.Cryptography.X509Certificates"/>

      <Reference name="Mono.Addins"/>

      <Reference name="Herbal3d.CommonEntitiesUtil"/>

      <Reference name="Nini"/>
      <Reference name="log4net"/>

      <Reference name="Fleck"/>
      <Reference name="Google.Protobuf"/>

      <Files>
        <Match pattern="*.cs" recurse="true">
          <Exclude name="Tests" pattern="Tests"/>
          <Exclude name="Object" pattern="obj"/>
        </Match>
      </Files>
    </Project>
